      ]
    },
    "frontend-e2e": {
      "tags": [],
      "implicitDependencies": [
        "frontend"
      ]
    },
    "api": {
      "tags": [
        "type:app",
        "scope:backend"
      ]
    },
    "frontend-about": {
      "tags": [
        "type:feature",
        "scope:frontend"
      ]
    },
    "frontend-shared-header": {
      "tags": [
        "type:feature",
        "scope:shared"
      ]
    },
    "frontend-shared-left-panel": {
      "tags": [
        "type:feature",
        "scope:shared"
      ]
    },
    "frontend-shared-data": {
      "tags": [
        "type:data",
        "scope:shared"
      ]
    },
    "frontend-shared-types": {
      "tags": [
        "shared"
      ]
    },
    "playground": {
      "tags": []
    },
    "frontend-shared-right-panel": {
      "tags": [
        "type:feature",
        "scope:shared"
      ]
    },
    "frontend-register": {
      "tags": [
        "type:feature",
        "scope:frontend"
      ]
    },
    "frontend-shared-auth": {
      "tags": [
        "type:shared",
        "scope:shared"
      ]
    },
    "frontend-store": {
      "tags": [
        "type:root-state",
        "scope:frontend"
      ]
    },
    "common-types": {
      "tags": [
        "type:common",
        "scope:common"
      ]
    }
  }
